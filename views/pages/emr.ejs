<%- layout('layouts/boilerplate') %>
    <link rel="stylesheet" href="/stylesheets/emr.css">

    <!-- hero section -->
    <div class="training-hero">
        <div class="training-container">
            <h1 class="training-heading font-semibold">Cybersecurity Training</h1>
            <h1 class="training-heading font-semibold">and Certifications</h1>
            <p class="training-description">Role-based content to prepare for exams,</p>
            <p class="training-description">certifications, or to train your entire workforce.</p>

            <div class="training-buttons flex gap-6">
                <button class="training-button button-1 font-semibold">For Individuals</button>
                <button class="training-button button-2 font-semibold">For Organizations</button>
            </div>
        </div>
    </div>

    <!-- fortune section -->
