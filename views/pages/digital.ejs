    <script>
        document.addEventListener("DOMContentLoaded", () => {
    // Slider 1: digital-auto-slider-2 (Moving to the right)
    const slider2 = document.getElementById("digital-auto-slider-2");
    const slider2Children = [...slider2.children];

    slider2Children.forEach((child) => {
        const clone = child.cloneNode(true);
        slider2.appendChild(clone);
    });

    let scrollAmount2 = 0;

    function scrollSlider2() {
        scrollAmount2 -= 1; // Moving to the right
        if (Math.abs(scrollAmount2) >= slider2Children[0].offsetWidth) {
            scrollAmount2 = 0;
            slider2.appendChild(slider2.children[0]);
        }
        slider2.style.transform = `translateX(${scrollAmount2}px)`;
        requestAnimationFrame(scrollSlider2);
    }

    scrollSlider2();

    // Slider 2: digital-auto-slider (Moving to the left)
    const slider1 = document.getElementById("digital-auto-slider");
    const slider1Children = [...slider1.children];

    slider1Children.forEach((child) => {
        const clone = child.cloneNode(true);
        slider1.appendChild(clone);
    });

    let scrollAmount1 = 0;

    function scrollSlider1() {
        scrollAmount1 += 1; // Moving to the left
        if (scrollAmount1 >= slider1Children[0].offsetWidth) {
            scrollAmount1 = 0;
            slider1.insertBefore(slider1.children[slider1.children.length - 1], slider1.children[0]);
        }
        slider1.style.transform = `translateX(${scrollAmount1}px)`;
        requestAnimationFrame(scrollSlider1);
    }

    scrollSlider1();
});

    </script>

