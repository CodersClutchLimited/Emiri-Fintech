    <script>
        // JavaScript for accordion functionality
        document.querySelectorAll('.respond-header').forEach(header => {
            header.addEventListener('click', () => {
                const allHeaders = document.querySelectorAll('.respond-header');
                const allBodies = document.querySelectorAll('.respond-body');

                // Close all other accordions
                allHeaders.forEach(h => h !== header && h.classList.remove('active'));
                allBodies.forEach(b => b !== header.nextElementSibling && (b.style.display = 'none'));

                // Toggle current accordion
                header.classList.toggle('active');
                const body = header.nextElementSibling;
                body.style.display = body.style.display === 'block' ? 'none' : 'block';
            });
        });
    </script>

