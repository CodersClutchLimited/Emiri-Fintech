            <!-- Circle 6 done -->
            <div class="purple-circles circle-6">
                <i class="ri-focus-fill"></i>
                <div class="dropdown-box z-50">
                    <div class="icon-container">
                        <img src="data:image/png;base64,"
                            alt="Icon" class="dropdown-icon">
                    </div>
                    <p class="location-text">Geneva, Lausanne & Zurich, CH</p>
                    <p class="street-name street1">Rue du Rhône, 1204 Geneva, Switzerlandt</p>
                    <p class="street-name street1">Avenue de la Gare, 1001 Lausanne, Switzerland</p>
                    <p class="street-name street1">Bahnhofstrasse, 8001 Zurich, Switzerland</p>
                </div>
            </div>
        </div>

    </div>

    <!-- find us container -->
    <div class="bg-white find-container mx-auto p-4">
        <!-- Outer Container -->
        <div class="find-section">
            <!-- About Us Paragraph -->
            <p class="map-about text-lg text-center font-semibold mb-3">Our locations</p>

            <!-- Title -->
            <h2 class="text-center text-4xl font-bold text-gray-900 mb-4">Visit our technology offices</h2>

