<%- layout('layouts/boilerplate') %>
<link rel="stylesheet" href="/stylesheets/industry.css">
<link rel="stylesheet" href="/stylesheets/augmented.css">
<link rel="stylesheet" href="/stylesheets/nft.css">

<div class="digital-hero">
    <div class="digital-texts">
        <div class=" title-1 font-bold text-5xl flex flex-col justify-center gap-7">
            <div class="image-digi flex gap-3"> <img src="/images/global-network.png" class="h-12" alt=""><span>Industries</span></div>
            <div class="white-r"></div>
        </div>
    </div>
</div>

<div class="acordion">
    <div class="industrie forensic-dropper">
        <div class=" dropper-container zero-pad">
            <div class="acord-starter">
                <div class="industrial core-container">
                    <div class="outdus core-tabs">
                        <div class="indus-tab core-tab font-semibold active" data-target="frontend">Finance & Banking</div>
                        <div class="indus-tab core-tab font-semibold" data-target="backend">Commerce & Retail</div>
                        <div class="indus-tab core-tab font-semibold" data-target="mobile">Healthcare</div>
                        <div class="indus-tab core-tab font-semibold" data-target="clouds">Education</div>
                        <div class="indus-tab core-tab font-semibold" data-target="erp-crm">Telecommunications</div>
                        <div class="indus-tab core-tab font-semibold" data-target="other">Business</div>
                        <div class="indus-tab core-tab font-semibold" data-target="automotive">Automotive</div>
                        <div class="indus-tab core-tab font-semibold" data-target="marketing">Marketing</div>
                        <div class="indus-tab core-tab font-semibold" data-target="public">Public Sector</div>
                        <div class="indus-tab core-tab font-semibold" data-target="real">Construction</div>
                        <div class="indus-tab core-tab font-semibold" data-target="agric">Environment</div>
                        <div class="indus-tab core-tab font-semibold" data-target="travel">Hospitality</div>
                    </div>
                    <div class="industrie core-content">
                        <div id="frontend" class="active">
                            <h2 class="font-semibold">Finance & Banking</h2>
                            <div class="tab-gridder grid grid-cols-3">
                                <div class="l-core-tech-item">
                                    <img class="h-9" src="https://cdn-icons-png.flaticon.com/128/10438/10438381.png"
                                        alt="React">
                                    <span>Finance</span>
                                </div>
                                <div class="l-core-tech-item">
                                    <img class="h-9" src="https://cdn-icons-png.flaticon.com/128/9359/9359415.png" alt="Angular">
                                    <span>Banking</span>
                                </div>
                                <div class="l-core-tech-item">
                                    <img class="h-9" src="https://cdn-icons-png.flaticon.com/128/10365/10365600.png" alt="Vue.js">
                                    <span>Trading</span>
                                </div>
                                <div class="l-core-tech-item">
                                    <img class="h-9" src="https://cdn-icons-png.flaticon.com/128/2603/2603735.png" alt="JavaScript">
                                    <span>Insurance</span>
                                </div>
                            </div>
                        </div>
    
                        <div id="backend">
                            <h2 class="font-semibold">Commerce & Retail</h2>
                            <div class="tab-gridder grid grid-cols-3">
                                <div style="white-space: nowrap;" class="l-core-tech-item">
                                    <img class="h-8" src="https://cdn-icons-png.flaticon.com/128/962/962649.png" alt="React">
                                    <span>eCommerce</span>
                                </div>
                                <div style="white-space: nowrap;" class="l-core-tech-item">
                                    <img class="h-8" src="https://cdn-icons-png.flaticon.com/128/2166/2166966.png" alt="Angular">
                                    <span>Retail</span>
                                </div>
                            </div>
                        </div>
    
                        <div id="mobile">
                            <h2 class="font-semibold">Healthcare</h2>
                            <div class="tab-gridder grid grid-cols-3">
                                <div style="white-space: nowrap;" class="l-core-tech-item">
                                    <img class="h-8" src="https://cdn-icons-png.flaticon.com/128/2382/2382533.png" alt="">
                                    <span>Medical care</span>
                                </div>
                                <div style="white-space: nowrap;" class="l-core-tech-item">
                                    <img class="h-8" src="https://cdn-icons-png.flaticon.com/128/4889/4889065.png" alt="">
                                    <span>Pharmaceutics</span>
                                </div>
                            </div>
                        </div>
    
                        <div id="clouds">
                            <h2 class="font-semibold">Education & Knowledge</h2>
                            <div class="tab-gridder grid grid-cols-3">
                                <div style="white-space: nowrap;" class="l-core-tech-item">
                                    <img src="https://cdn-icons-png.flaticon.com/128/16811/16811637.png" class="h-8" alt="">
                                    <span>Education</span>
                                </div>
                            </div>
                        </div>
    
                        <div id="erp-crm">
                            <h2 class="font-semibold">ERP & CRM</h2>
                            <div class="tab-gridder grid grid-cols-3">
                                <div style="white-space: nowrap;" class="l-core-tech-item">
                                    <img src="https://cdn-icons-png.flaticon.com/128/2649/2649792.png" class="h-8" alt="">
                                    <span>Telecom</span>
                                </div>
                                <div style="white-space: nowrap;" class="l-core-tech-item">
                                    <img src="https://cdn-icons-png.flaticon.com/128/4187/4187336.png" class="h-7" alt="">
                                    <span>Entertainment</span>
                                </div>
                            </div>
                        </div>
    
                        <div id="other">
                            <h2 class="font-semibold">Business & Industry</h2>
                            <div class="tab-gridder grid grid-cols-3">
                                <div style="white-space: nowrap;" class="l-core-tech-item">
                                    <img src="https://cdn-icons-png.flaticon.com/128/17258/17258901.png" class="h-8" alt="">
                                    <span>Enterprise</span>
                                </div>
                                <div style="white-space: nowrap;" class="l-core-tech-item">
                                    <img src="https://cdn-icons-png.flaticon.com/128/2640/2640844.png" class="h-8" alt="">
                                    <span>Manufacturing</span>
                                </div>
                            </div>
                        </div>

                        <div id="automotive">
                            <h2 class="font-semibold">Automotive & Transportation</h2>
                            <div class="tab-gridder grid grid-cols-3">
                                <div style="white-space: nowrap;" class="l-core-tech-item">
                                    <img src="https://cdn-icons-png.flaticon.com/128/3577/3577867.png" class="h-8" alt="">
                                    <span>Automotive</span>
                                </div>
                                <div style="white-space: nowrap;" class="l-core-tech-item">
                                    <img src="https://cdn-icons-png.flaticon.com/128/4363/4363708.png" class="h-8" alt="">
                                    <span>Logistics</span>
                                </div>
                                <div style="white-space: nowrap;" class="l-core-tech-item">
                                    <img src="https://cdn-icons-png.flaticon.com/128/826/826070.png" class="h-8" alt="">
                                    <span>Transportation</span>
                                </div>
                            </div>
                        </div>

                        <div id="marketing">
                            <h2 class="font-semibold">Marketing & Advertising</h2>
                            <div class="tab-gridder grid grid-cols-3">
                                <div style="white-space: nowrap;" class="l-core-tech-item">
                                    <img src="https://cdn-icons-png.flaticon.com/128/3211/3211596.png" class="h-8" alt="">
                                    <span>Marketing</span>
                                </div>
                                <div style="white-space: nowrap;" class="l-core-tech-item">
                                    <img src="https://cdn-icons-png.flaticon.com/128/12337/12337120.png" class="h-8" alt="">
                                    <span>Advertising</span>
                                </div>
                            </div>
                        </div>

                        <div id="public">
                            <h2 class="font-semibold">Public Sector & Utilities</h2>
                            <div class="tab-gridder grid grid-cols-3">
                                <div style="white-space: nowrap;" class="l-core-tech-item">
                                    <img src="https://cdn-icons-png.flaticon.com/128/18591/18591577.png" class="h-8" alt="">
                                    <span>Government</span>
                                </div>
                                <div style="white-space: nowrap;" class="l-core-tech-item">
                                    <img src="https://cdn-icons-png.flaticon.com/128/11741/11741971.png" class="h-8" alt="">
                                    <span>Energy & Utilities</span>
                                </div>
                            </div>
                        </div>

                        <div id="real">
                            <h2 class="font-semibold">Construction & Real Estate</h2>
                            <div class="tab-gridder grid grid-cols-3">
                                <div style="white-space: nowrap;" class="l-core-tech-item">
                                    <img src="https://cdn-icons-png.flaticon.com/128/4490/4490381.png" class="h-8" alt="">
                                    <span>Construction</span>
                                </div>
                                <div style="white-space: nowrap;" class="l-core-tech-item">
                                    <img src="https://cdn-icons-png.flaticon.com/128/2238/2238337.png" class="h-8" alt="">
                                    <span>Real Estate</span>
                                </div>
                            </div>
                        </div>

                        <div id="agric">
                            <h2 class="font-semibold">Agriculture & Environment</h2>
                            <div class="tab-gridder grid grid-cols-3">
                                <div style="white-space: nowrap;" class="l-core-tech-item">
                                    <img src="https://cdn-icons-png.flaticon.com/128/15730/15730298.png" class="h-8" alt="">
                                    <span>Agriculture</span>
                                </div>
                            </div>
                        </div>

                        <div id="travel">
                            <h2 class="font-semibold">Travel & Hospitality</h2>
                            <div class="tab-gridder grid grid-cols-3">
                                <div style="white-space: nowrap;" class="l-core-tech-item">
                                    <img src="https://cdn-icons-png.flaticon.com/128/15490/15490223.png" class="h-8" alt="">
                                    <span>Travel</span>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script src="/javascripts/augmented.js"></script>